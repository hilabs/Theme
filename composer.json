{
    "name": "Hilabs/Theme",
    "type": "library",
    "description": "Laravel Theme",
    "keywords": [
        "Hilabs",
        "Theme"
    ],
    "homepage": "https://github.com/Hilabs/Theme",
    "license": "MIT",
    "authors": [
        {
            "name": "Shandy Hermawan",
            "email": "hello@sandihermawan.link",
            "homepage": "sandihermawan.link",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Hilabs\\Theme\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Hilabs\\Theme\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
